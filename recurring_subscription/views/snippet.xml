<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="products_category_wise_template" name="Best Seller">
        <!--        Template Best seller product dynamic snippet-->
        <t t-name="home_template">
            <section class="best_seller_product_snippet">
                <div id="last_four_credits"/>
            </section>
        </t>
    </template>
    <template id="basic_snippet" inherit_id="website.snippets"
              name="Best seller Snippet">
        <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]"
               position="after">
            <t t-snippet="recurring_subscription.products_category_wise_template"/>
        </xpath>
    </template>
</odoo>