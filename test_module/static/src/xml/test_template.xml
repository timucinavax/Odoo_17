<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
   <t t-name="test_module.Counter">
      <div>
      <p>Counter: <t t-esc="state.value"/></p>
          <t t-esc="state.product"/>
          <br/>
          <button class="btn btn-primary"
                  t-on-click="increment">Increment</button>
          <button id="decbtn" class="btn btn-primary" t-on-click="decrement">Decrement</button>
          <ChildComp state="state.value"/>
      </div>
   </t>
</templates>